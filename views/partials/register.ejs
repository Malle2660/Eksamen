<div class="auth-form" id="registerForm">
    <h2>Opret Bruger</h2>
    <form id="register">
        <input type="text" id="reg-username" placeholder="Brugernavn" required>
        <input type="email" id="reg-email" placeholder="Email" required>
        <input type="password" id="reg-password" placeholder="Adgangskode" required>
        <button type="submit">Opret Konto</button>
    </form>
</div> 